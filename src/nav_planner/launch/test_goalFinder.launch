<launch>
    <node pkg="octomap_server" type="octomap_server_node" name="octomap_server">
		<param name="resolution" value="0.05" />
		<param name="frame_id" type="string" value="odom_combined" />
		<param name="sensor_model/max_range" value="10.0" />
		
		<remap from="cloud_in" to="/narrow_stereo/points_filtered2" />
	</node>
    
    <node pkg="nav_planner" type="goal_identifier_node" name="goal_identifier">
		<remap from="octomap" to="/octomap_full" />
        <remap from="odometry" to="" />
	</node>
</launch>

